public class DHayvanlar {
    protected void hareket( ){
        System.out.println("Hayvanlar hareket ederler");
    }
    protected void solunum( ) {
        System.out.println("Hayvanlar nefes alirlar");
    }
    protected void beslenme( ) {
        System.out.println("Hayvanlar beslenirler");
    }
    protected void cogalma( ) {
        System.out.println("Hayvanlar cogalirlar");
    }
    protected void omur( ) {
        System.out.println("Hayvanlar yasar ve olurler");
    }
}